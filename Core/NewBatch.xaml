<Window x:Class="ActivityScheduler.Core.NewBatch"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ActivityScheduler.Core"
        mc:Ignorable="d"
        Title="New Batch" Height="220" Width="425"
        ResizeMode="NoResize">
    <Grid>
        <Button Content="Create batch" Name="BatchCreate" HorizontalAlignment="Left" Margin="305,24,0,0" VerticalAlignment="Top" Width="99" Click="BatchCreate_Click" />
        <Button Content="Cancel" x:Name="Cancel" HorizontalAlignment="Left" Margin="305,59,0,0" VerticalAlignment="Top" Width="99" Click="Cancel_Click" />
    </Grid>

</Window>
